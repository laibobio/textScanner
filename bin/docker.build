    echo "Run training with proxy"
    docker build \
        --build-arg HTTP_PROXY="http://172.17.0.1:8123" \
        --build-arg HTTPS_PROXY="http://172.17.0.1:8123" \
        -f config/Dockerfile -t textscanner.img .
